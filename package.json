{
  "name": "bubbles-ui",
  "version": "0.10.0-next.0",
  "description": "Opinionated Svelte Kit UI Components",
  "scripts": {
    "dev": "vite dev --port 3001",
    "watch": "svelte-package --watch",
    "build": "vite build",
    "preview": "vite preview",
    "package": "svelte-kit sync && svelte-package",
    "publish": "npm install && npm run package && npm publish --tag next",
    "publish:prod": "npm install && svelte-kit package && npm publish --tag latest"
  },
  "devDependencies": {
    "@sveltejs/adapter-vercel": "^2.4.1",
    "@sveltejs/package": "^2.0.2",
    "@sveltejs/svelte-scroller": "^2.0.7",
    "autoprefixer": "^10.4.13",
    "mdsvex": "^0.10.6",
    "prettier": "^2.6.1",
    "prettier-plugin-svelte": "^2.6.0",
    "rollup-plugin-inline-svg": "^2.0.0",
    "rollup-plugin-svg": "^2.0.0",
    "svelte-highlight": "^5.1.3",
    "svelte-preprocess": "^4.10.6",
    "svelte2tsx": "^0.6.0",
    "vite": "^4.0.3"
  },
  "dependencies": {
    "@fontsource/fira-mono": "^4.5.0",
    "@fontsource/inter": "^4.5.1",
    "@fontsource/poppins": "^4.5.0",
    "@lukeed/uuid": "^2.0.0",
    "@stripe/stripe-js": "^1.22.0",
    "@sveltejs/kit": "^1.14.0"
  },
  "peerDependencies": {
    "svelte": "^3.54.0"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/vpanyushenko/bubbles.git"
  },
  "author": "Valentin Panyushenko",
  "bugs": {
    "url": "https://github.com/vpanyushenko/bubbles/issues"
  },
  "homepage": "https://bubbles-pied.vercel.app/",
  "files": [
    "dist"
  ],
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "svelte": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./*": "./dist/*"
  }
}
